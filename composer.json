{
  "name": "poseso/laravel-settings",
  "description": "Persistent settings for Laravel Framework",
  "keywords": [
    "poseso",
    "laravel",
    "settings"
  ],
  "homepage": "https://github.com/poseso/laravel-settings",
  "license": "MIT",
  "authors": [
    {
      "name": "Carlos Sanchez",
      "email": "carlosmsanchez@gmail.com",
      "homepage": "http://www.blacklyontech.com",
      "role": "Package Developer"
    }
  ],
  "require": {
    "php": "^7.1",
    "ext-json": "*",
    "illuminate/contracts": "^5.5|^6.0",
    "illuminate/database": "^5.5|^6.0",
    "illuminate/events": "^5.5|^6.0",
    "illuminate/support": "^5.5|^6.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.0",
    "orchestra/testbench": "^3.6|^4.0",
    "phpunit/phpunit": "^6.1|^7.0|^8.0"
  },
  "autoload": {
    "files": [
      "src/helpers.php"
    ],
    "psr-4": {
      "Poseso\\Settings\\": "src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Poseso\\Settings\\ServiceProvider"
      ],
      "aliases": {
        "Settings": "Poseso\\Settings\\Facades\\SettingsFacade"
      }
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Poseso\\Settings\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit --colors=always"
  },
  "config": {
    "sort-packages": true
  }
}
